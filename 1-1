#include <stdio.h>

int input(int *a, int *n);
void output(int *a, int *n);
void bubble_sort(int *a, int *n);

int main(){
int n = 10, a[10];
input(a, &n);
bubble_sort(a, &n);
output(a, &n);
return 0;
}












int input(int *a, int *n){
    for (int i = 0; i < *n; i++)
    {
        char o;
        scanf("%d%c", &a[i] , &o);
    }
    return 0;
}


 
void output (int *a, int *n){
    for (int i = 0; i < *n; i++)
    {   printf("%d ", a[i]);
    }
}





void bubble_sort(int *a, int *n){
for(int i = 0; i < *n - 1; i++)
{
    for(int j = 0 ; j < *n - i - 1; j++)
    {
        if(a[j] > a[j+1])
        {
            int tmp = a[j];
            a[j] = a[j+1];
            a[j+1] = tmp;
        }
    }
}
}
